{
  "name": "@microdrop/device-ui-plugin",
  "version": "0.1.12",
  "description": "UI Plugin to view device, routes, and electrodes",
  "main": "device-ui-plugin.web.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "build-dev": " npm run-script build-device-controller && npm run-script link-packages && webpack",
    "link-packages": "npm ln ../device-controller ../microdrop-async ../ui-plugin",
    "build-device-controller": "npm run-script --prefix ../device-controller build-dev",
    "update-deps": "ncu -ua @microdrop/async @microdrop/device-controller @microdrop/ui-plugin && npm install",
    "git-add": "git add package-lock.json && git add -p . && git commit",
    "publish-pkg": "npm run build && npm version patch && npm publish && git push origin master"
  },
  "keywords": [
    "microdrop",
    "device-ui-plugin",
    "ui-plugin"
  ],
  "author": "Sci-Bots",
  "license": "BSD-2-Clause",
  "dependencies": {
    "@microdrop/async": "0.2.18",
    "@microdrop/device-controller": "^0.4.9",
    "@microdrop/ui-plugin": "^0.1.4",
    "async-mqtt": "^1.0.1",
    "dat.gui": "^0.6.1",
    "jquery": "^3.2.1",
    "jquery-contextmenu": "^2.6.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "css-loader": "^0.28.7",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2"
  }
}
